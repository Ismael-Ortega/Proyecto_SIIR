@{
    ViewData["Title"] = "Lista de Coaches";
}

<div class="container mt-4">
    <div class="row">
        <div class="col-8">
            <h2 class="display-2">Coaches</h2>
        </div>
        <div class="col-4 text-right">
            <a asp-area="Identity" asp-page="/Account/Register" class="btn btn-primary">
                <i class="fas fa-plus"></i> Dar de alta nuevo Estudiante
            </a>
        </div>
    </div>

    <br />
    <br />

    <table id="tblCategoriasCoach" class="table table-striped table-bordered" style="width: 100%">
        <thead>
            <tr>
                <th>Id</th>
                <th>Nombre</th>
                <th>Apellido Paterno</th>
                <th>Apellido Materno</th>
                <th>Acciones</th>
            </tr>
        </thead>

    </table>
</div>

@section Scripts{
    <script src="~/js/coach.js"></script>
}