@{
    ViewData["Title"] = "Lista de Representativos";
}

<div class="container mt-4">
    <div class="row">
        <div class="col-8">
            <h2 class="text-primary">Lista de grupos Representativos</h2>
        </div>
    </div>
    <div class="col-4 text-right">
        <a asp-action="Create" class="btn btn-primary"><i class="fas fa-plus"></i>Crear nuevo grupo representativo</a>
    </div>
    <br/>
    <br/>
    <table id="tblRepresentatives" class="table table-striped table-bordered" style="width:100%">
        <thead>
            <tr>
                <th>Id</th>
                <th>Nombre</th>
                <th>Categoria</th>
                <th>Acciones</th>
            </tr>
        </thead>

    </table>

</div>

@section Scripts{
    <script src="~/js/representative.js"></script>
}