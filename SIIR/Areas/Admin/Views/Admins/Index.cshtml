@{
    ViewData["Title"] = "Lista de Administradores";
}

<div class="container-fluid mt-4">
    <div class="row">
        <div class="col-8">
            <h1 class="display-4">Administradores</h1>
        </div>
    </div>
    <br />
    <br />
    <div class="table-responsive">
        <table id="tblAdmins" class="table table-striped table-bordered" style="width:100%">
            <thead>
                <tr>
                    <th>Nombre</th>
                    <th>Apellido Paterno</th>
                    <th>Apellido Materno</th>
                </tr>
            </thead>
        </table>
    </div>
</div>


@section Scripts {
    <script src="~/js/admins.js"></script>
}